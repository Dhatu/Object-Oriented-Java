b) 

public Book(String t, String a, String p){ 
title = t; 
author = a; 
publisher = p; 
copiesSold = 0; 
} 


public Book(String t, String a, String p, int c){ 
title = t; 
author = a; 
publisher = p; 
copiesSold = c; 
} 

public String getTitle(){ 
return title; 
} 

public void setCopiesSold(int c){ 
copiesSold = c; 
} 